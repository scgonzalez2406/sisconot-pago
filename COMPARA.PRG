CLOSE DATA
SET TALK OFF
SET DELE ON
SELECT 1
USE MA021999 ORDER CED
SELECT 2
USE MP021999 ORDER CED
SELECT 3
USE DATOS ORDER CEDULA
SELECT 1
GO TOP
XCED=" "
LI=1
DO WHILE .NOT. EOF()
   XCED=CED
   
   SELECT 2
   GO TOP
   IF .NOT. SEEK(XCED)
      SELECT 3
      IF SEEK(XCED)
         @LI,01 SAY XCED
         @LI,15 SAY NOMBRES
         LI=LI+1
      ELSE
         @LI,01 SAY XCED
         @LI,15 SAY "SIN NOMBRE"
      ENDIF
   ENDIF
   SELECT 1
   SKIP
ENDDO
CLOSE DATA